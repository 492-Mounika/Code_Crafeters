S_0x5bd35ddae2e0 .scope module, "tb_graycoder" "tb_graycoder" 2 3;
 .timescale -9 -12;
v0x5bd35ddcb520_0 .var "bin_in", 3 0;
v0x5bd35ddcb610_0 .var "clk", 0 0;
v0x5bd35ddcb6e0_0 .net "gray_out", 3 0, v0x5bd35ddcb3e0_0;  1 drivers
S_0x5bd35ddae470 .scope task, "check_gray" "check_gray" 2 27, 2 27 0, S_0x5bd35ddae2e0;
 .timescale -9 -12;
v0x5bd35dd75ba0_0 .var "binary", 3 0;
v0x5bd35ddcaef0_0 .var "expected", 3 0;
E_0x5bd35ddacb80 .event posedge, v0x5bd35ddcb320_0;
E_0x5bd35dd74670 .event negedge, v0x5bd35ddcb320_0;
TD_tb_graycoder.check_gray ;
    %wait E_0x5bd35dd74670;
    %load/vec4 v0x5bd35dd75ba0_0;
    %store/vec4 v0x5bd35ddcb520_0, 0, 4;
    %wait E_0x5bd35ddacb80;
    %delay 1000, 0;
    %load/vec4 v0x5bd35ddcb6e0_0;
    %load/vec4 v0x5bd35ddcaef0_0;
    %cmp/ne;
    %jmp/0xz  T_0.0, 6;
    %vpi_call 2 34 "$display", "[FAIL] Time: %0t | Input = %b | Expected = %b | Got = %b", $time, v0x5bd35dd75ba0_0, v0x5bd35ddcaef0_0, v0x5bd35ddcb6e0_0 {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 36 "$display", "[PASS] Time: %0t | Input = %b | Output = %b", $time, v0x5bd35dd75ba0_0, v0x5bd35ddcb6e0_0 {0 0 0};
T_0.1 ;
    %end;
S_0x5bd35ddcafd0 .scope module, "dut" "graycoder" 2 10, 3 1 0, S_0x5bd35ddae2e0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 4 "bin_in";
    .port_info 2 /OUTPUT 4 "gray_out";
v0x5bd35ddcb220_0 .net "bin_in", 3 0, v0x5bd35ddcb520_0;  1 drivers
v0x5bd35ddcb320_0 .net "clk", 0 0, v0x5bd35ddcb610_0;  1 drivers
v0x5bd35ddcb3e0_0 .var "gray_out", 3 0;
    .scope S_0x5bd35ddcafd0;
T_1 ;
    %wait E_0x5bd35ddacb80;
    %load/vec4 v0x5bd35ddcb220_0;
    %parti/s 1, 3, 3;
    %ix/load 4, 3, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x5bd35ddcb3e0_0, 4, 5;
    %load/vec4 v0x5bd35ddcb3e0_0;
    %parti/s 1, 3, 3;
    %load/vec4 v0x5bd35ddcb220_0;
    %parti/s 1, 2, 3;
    %xor;
    %ix/load 4, 2, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x5bd35ddcb3e0_0, 4, 5;
    %load/vec4 v0x5bd35ddcb3e0_0;
    %parti/s 1, 2, 3;
    %load/vec4 v0x5bd35ddcb220_0;
    %parti/s 1, 1, 2;
    %xor;
    %ix/load 4, 1, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x5bd35ddcb3e0_0, 4, 5;
    %load/vec4 v0x5bd35ddcb3e0_0;
    %parti/s 1, 1, 2;
    %load/vec4 v0x5bd35ddcb220_0;
    %parti/s 1, 0, 2;
    %xor;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x5bd35ddcb3e0_0, 4, 5;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5bd35ddae2e0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5bd35ddcb610_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x5bd35ddae2e0;
T_3 ;
    %delay 5000, 0;
    %load/vec4 v0x5bd35ddcb610_0;
    %inv;
    %store/vec4 v0x5bd35ddcb610_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5bd35ddae2e0;
T_4 ;
    %vpi_call 2 22 "$dumpfile", "graycoder.vcd" {0 0 0};
    %vpi_call 2 23 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5bd35ddae2e0 {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x5bd35ddae2e0;
