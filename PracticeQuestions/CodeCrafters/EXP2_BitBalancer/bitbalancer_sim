S_0x61c40f268d90 .scope module, "tb_bitbalancer" "tb_bitbalancer" 2 2;
 .timescale -9 -12;
v0x61c40f288320_0 .var "clk", 0 0;
v0x61c40f2883e0_0 .net "count", 3 0, v0x61c40f288040_0;  1 drivers
v0x61c40f2884b0_0 .var "in", 7 0;
v0x61c40f2885b0_0 .var "reset", 0 0;
S_0x61c40f268f20 .scope task, "apply_test" "apply_test" 2 24, 2 24 0, S_0x61c40f268d90;
 .timescale -9 -12;
v0x61c40f22eba0_0 .var "expected", 3 0;
v0x61c40f25a950_0 .var "val", 7 0;
E_0x61c40f2659e0 .event posedge, v0x61c40f287f60_0;
TD_tb_bitbalancer.apply_test ;
    %load/vec4 v0x61c40f25a950_0;
    %store/vec4 v0x61c40f2884b0_0, 0, 8;
    %wait E_0x61c40f2659e0;
    %delay 1000, 0;
    %load/vec4 v0x61c40f2883e0_0;
    %load/vec4 v0x61c40f22eba0_0;
    %cmp/ne;
    %jmp/0xz  T_0.0, 6;
    %vpi_call 2 30 "$display", "Test failed for input %b: Expected = %0d, Got = %0d", v0x61c40f25a950_0, v0x61c40f22eba0_0, v0x61c40f2883e0_0 {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 32 "$display", "Test passed for input %b: Count = %0d", v0x61c40f25a950_0, v0x61c40f2883e0_0 {0 0 0};
T_0.1 ;
    %end;
S_0x61c40f287980 .scope module, "uut" "bitbalancer" 2 13, 3 1 0, S_0x61c40f268d90;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 8 "in";
    .port_info 3 /OUTPUT 4 "count";
v0x61c40f287f60_0 .net "clk", 0 0, v0x61c40f288320_0;  1 drivers
v0x61c40f288040_0 .var "count", 3 0;
v0x61c40f288120_0 .net "in", 7 0, v0x61c40f2884b0_0;  1 drivers
v0x61c40f2881e0_0 .net "reset", 0 0, v0x61c40f2885b0_0;  1 drivers
S_0x61c40f287b80 .scope function.vec4.s4, "count_ones" "count_ones" 3 8, 3 8 0, S_0x61c40f287980;
 .timescale 0 0;
; Variable count_ones is vec4 return value of scope S_0x61c40f287b80
v0x61c40f287dc0_0 .var/i "i", 31 0;
v0x61c40f287ea0_0 .var "val", 7 0;
TD_tb_bitbalancer.uut.count_ones ;
    %pushi/vec4 0, 0, 4;
    %ret/vec4 0, 0, 4;  Assign to count_ones (store_vec4_to_lval)
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x61c40f287dc0_0, 0, 32;
T_1.2 ;
    %load/vec4 v0x61c40f287dc0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_1.3, 5;
    %retload/vec4 0; Load count_ones (draw_signal_vec4)
    %load/vec4 v0x61c40f287ea0_0;
    %load/vec4 v0x61c40f287dc0_0;
    %part/s 1;
    %pad/u 4;
    %add;
    %ret/vec4 0, 0, 4;  Assign to count_ones (store_vec4_to_lval)
    %load/vec4 v0x61c40f287dc0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x61c40f287dc0_0, 0, 32;
    %jmp T_1.2;
T_1.3 ;
    %end;
    .scope S_0x61c40f287980;
T_2 ;
    %wait E_0x61c40f2659e0;
    %load/vec4 v0x61c40f2881e0_0;
